{
  "expo": {
    "name": "Chatbot",
    "slug": "ai-poweredchatbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bot.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bot.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sahilmalgundkar.aipoweredchatbot"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/bot.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bot.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1D202D"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "921d14e7-98e6-4c79-88e3-ee3bb69f3f8c"
      }
    }
  }
}
